    h1 test  
    each pi in RPIs
        h1(id='nom' + pi.socket.index) nom : #{pi.name}
        h1(id='volume' + pi.socket.index) volume : #{pi.volume}
        input(id='input' + pi.socket.index)     
        button(onclick='changeVolume('+ pi.socket.index + ')', type='button') change volume
        hr
            
  script(src='https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.0.3/socket.io.js')
  script(src='/js/sockets.js')
  
